{
	"name": "mp-cli",
	"version": "1.0.0",
	"description": "一款小而美的微信小程序脚手架工具，帮助你快速开发微信小程序应用",
	"main": "index.js",
	"bin": {
		"mp": "./bin/mp",
		"precommit": "lint-staged"
	},
	"dependencies": {
		"@moyuyc/inquirer-autocomplete-prompt": "^1.3.2",
		"chalk": "^4.1.0",
		"commander": "^7.0.0",
		"cross-spawn": "^7.0.3",
		"download-git-repo": "^3.0.2",
		"fs": "0.0.1-security",
		"fuzzy": "^0.1.3",
		"inquirer": "^7.3.3",
		"ora": "^5.3.0",
		"os": "^0.1.1",
		"path": "^0.12.7",
		"readline": "^1.3.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.18.2",
		"husky": "^6.0.0",
		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1"
	},
	"scripts": {
		"clear": "rm -rf ./dist"
	},
	"author": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/szh0526/mp-cli.git"
	},
	"husky": {
		"hooks": {
			"commit-msg": "git-hooks-check && eslint --ext .js,.vue src noConsole"
		}
	},
	"lint-staged": {
		"**.js": [
			"eslint --fix --ext .js",
			"prettier --write",
			"git add"
		]
	},
	"templates": {
		"base": {
			"url": "https://github.com/szh0526/mp-cli-template-base/#main",
			"default": 1
		}
	},
	"projects":[{
		"appName":"万店宝小程序",
		"git":"https://git.jd.com/wjfe/wdb-wx"
	}]
}
